<div class="admin-card card event-card">
	<a href="/event/{{_id}}">
		<div class="img" style="background-image:url({{eventImage}})" ></div>
		<div class="text-wrapper">
			<h5>{{title}}</h5>
			<div class="location">
				{{location}}
			</div>
			<div class="date">
				{{date.from}}

			</div>
			<a href="/admin-events/{{_id}}/edit" class="edit-link">Edit</a>
			<button class="btn-delete-event btn" id="{{_id}}">Delete</button>
		</div>
	</a>

</div>

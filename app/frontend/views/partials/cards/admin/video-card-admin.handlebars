
<div class="card admin-card video-card ">
	<div id="{{ _id }}" class="link openpop-video">
		<div class="img" style="background-image:url({{image}})">
			{{#if videoLength }}
			<div class="video-length">
				{{ videoLength }}
			</div>
			{{/if}}
		</div>
		<div class="text-wrapper">
			<h5>{{title}}</h5>
		</div>
	</div>

	<a href="/admin-videos/{{_id}}/edit">Edit</a>
	<button class="btn-delete-video"id="{{_id}}">Delete</button>
</div>


<div class="card speaker-card">
	<div id="{{ _id }}" class="link openpop">
		<div class="img" style="background-image: url({{ imageUrl }})"></div>

		<div class="text-wrapper">
			<div class="speaker-name">
				<h5>{{ fullName }}</h5>
			</div>
			<div class="job-title">{{ profession }}</div>
			<div class="location">[{{ location }}]</div>
			<div class="nationality" style="background-image: url({{ nationality }})"></div>
		</div>
	</div>
</div>


<div id="modalID{{ _id }}" class="modal">
	<div class="modal-content">
		<!-- The Modal Content -->
	</div>
	<div class="button-close-container">
		<div class="button-close">Close</div>
	</div>
</div>
